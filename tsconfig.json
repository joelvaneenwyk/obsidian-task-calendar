{
	"compilerOptions": {
		"baseUrl": ".",
		"inlineSourceMap": true,
		"inlineSources": true,
		"module": "ESNext",
		"target": "ES6",
		"allowJs": true,
		"noImplicitAny": true,
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"importHelpers": true,
		"isolatedModules": true,
		"strictNullChecks": true,
		"jsx": "react-jsx",
		"lib": [
			"DOM",
			"ES5",
			"ES6",
			"ES7",
			"es2021",
		],
		"paths": {
			"@task-calendar-views/*": [
				"views/obsidian-task-timeline-view/src/*"
			]
		}
	},
	"include": [
		"**/*.ts",
		"views/obsidian-task-timeline-view/src/components/asserts/icons.tsx",
		"src/views.tsx"
	]
}
