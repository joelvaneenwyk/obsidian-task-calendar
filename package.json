{
  "name": "mycoshiro-task-calendar",
  "version": "0.2.3",
  "description": "This is a simple wrapper for Obsidian-Tasks-Calendar (https://github.com/702573N/Obsidian-Tasks-Calendar) and Obsidian-Tasks-Timeline (https://github.com/702573N/Obsidian-Tasks-Timeline).",
  "main": "src/main.ts",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "node esbuild.config.mjs",
    "check": "tsc -noEmit -skipLibCheck && yarn shx echo \"No issues found.\"",
    "deploy": "cross-env-shell \"yarn shx --verbose mkdir -p \"${PROJECT_CWD}/.obsidian/plugins/${npm_package_name}\" && yarn shx --verbose cp dist/main.js manifest.json versions.json \"${PROJECT_CWD}/.obsidian/plugins/${npm_package_name}\" \"",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "tsc -noEmit -skipLibCheck && eslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\" \"src/**/*.tsx\"",
    "release": "yarn check && yarn build && yarn deploy && yarn shx echo \"Deployment complete.\""
  },
  "keywords": [
    "calendar",
    "obsidian"
  ],
  "author": "Joel Van Eenwyk",
  "authorUrl": "https://github.com/joelvaneenwyk/",
  "repository": {
    "type": "git",
    "url": "https://github.com/joelvaneenwyk/mycoshiro-task-calendar.git"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.23.4",
    "@codemirror/state": "^6.0.0",
    "@codemirror/view": "^6.0.0",
    "@svgr/webpack": "^8.1.0",
    "@types/backbone": "^1.4.18",
    "@types/jest": "^29.5.10",
    "@types/luxon": "^3.3.5",
    "@types/node": "^20.10.0",
    "@types/parsimmon": "^1.10.9",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-import-glob": "^2.0.0",
    "backbone": "^1.5.0",
    "builtin-modules": "3.3.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "emoji-regex": "^10.3.0",
    "esbuild": "0.19.7",
    "esbuild-plugin-import-glob": "^0.1.1",
    "esbuild-plugin-replace": "^1.4.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-json-files": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^2.1.1",
    "eslint-plugin-svelte": "^2.35.1",
    "eslint-plugin-unicorn": "^49.0.0",
    "jest": "^29.7.0",
    "luxon": "^3.4.4",
    "moment": "^2.29.4",
    "obsidian": "latest",
    "parsimmon": "^1.18.1",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shx": "^0.3.4",
    "svelte": "^4.2.7",
    "tslib": "2.6.2",
    "typescript": "5.3.2",
    "unified-lint-rule": "^2.1.2"
  }
}
